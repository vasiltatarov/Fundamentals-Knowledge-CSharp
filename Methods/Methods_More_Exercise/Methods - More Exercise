1.
using System;
using System.ComponentModel;
using System.Diagnostics.CodeAnalysis;
using System.Linq;
using System.Runtime.ExceptionServices;
using System.Runtime.InteropServices;
using System.Security.Cryptography;
using System.Transactions;

namespace RealWorldExample
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] command = Console.ReadLine().Split(" ").ToArray();

            GetTypes(command);
        }

        static void GetTypes(string[] command)
        {
            if (command[0] == "int")
            {
                int number = int.Parse(Console.ReadLine());
                Console.WriteLine(number * 2);
            }
            else if (command[0] == "real")
            {
                double number = double.Parse(Console.ReadLine());
                Console.WriteLine($"{number * 1.5:F2}");
            }
            else if (command[0] == "string")
            {
                string text = Console.ReadLine();
                Console.WriteLine($"${text}$");
            }
        }
    }
}

4.
static void Main(string[] args)
        {
            int num  = int.Parse(Console.ReadLine());

            if (num <= 0)
            {
                Console.WriteLine(0);
            }
            else if (num == 1)
            {
                Console.Write(1);
            }
            else if (num == 2)
            {
                Console.Write("1 1");
            }
            else if (num == 3)
            {
                Console.Write("1 1 2");
            }
            else
            {
                Console.Write("1 1 2 ");
                GetTribonacci(num);
            }
        }

        private static void GetTribonacci(int num)
        {
            int minus3 = 1;
            int minus2 = 1;
            int minus1 = 2;
            int max = num;
            for (int i = 1; i <= max - 3; i++)
            {
                num = minus3 + minus2 + minus1;
                minus3 = minus2;
                minus2 = minus1;
                minus1 = num;
                Console.Write("{0} ", num);
            }
        }
        
5.
static void Main(string[] args)
        {
            //int arrayElements = int.Parse(Console.ReadLine());
            int[] numbers = new int[3];

            for (int i = 0; i < numbers.Length; i++)
            {
                numbers[i] = int.Parse(Console.ReadLine());
            }

            Console.WriteLine(CheckPositiveOrNegative(numbers));

        }

        static string CheckPositiveOrNegative(int[] numbers)
        {
            string positive = "positive";
            string negative = "negative";
            string zero = "zero";

            int zeroIndex = 0;
            int positiveIndex = 0;
            int negativeIndex = 0;

            for (int i = 0; i < numbers.Length; i++)
            {
                if (numbers[i] == 0)
                {
                    zeroIndex++;
                }
                if (numbers[i] < 0)
                {
                    negativeIndex++;
                }
                else if (numbers[i] > 0)
                {
                    positiveIndex++;
                }
            }

            if (zeroIndex >= 1)
            {
                return zero;
            }
            else if (positiveIndex == 3 || positiveIndex == 1)
            {
                return positive;
            }
            else
            {
                return negative;
            }
        }
