            int n = int.Parse(Console.ReadLine());

            for (int col = 0; col < n; col++)
            {
                int number = 1;

                for (int row = 0; row <= col; row++)
                {
                    Console.Write(number + " ");
                    number = number * (col - row) / (row + 1);
                }

                Console.WriteLine();
            }
