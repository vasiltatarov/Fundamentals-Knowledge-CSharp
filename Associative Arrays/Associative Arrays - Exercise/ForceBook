                                                          //60/100!!!!!!!!!!!
            var forceBook = new SortedDictionary<string, List<string>>();

            string command = Console.ReadLine();
            while (command != "Lumpawaroo")
            {
                if (command.Contains(" | "))
                {
                    string[] line = command.Split(" | ");
                    string forceSide = line[0];
                    string forceUser = line[1];

                    if (forceBook.ContainsKey(forceSide))
                    {
                        if (!forceBook[forceSide].Contains(forceUser))
                        {
                            forceBook[forceSide].Add(forceUser);
                        }
                    }
                    else if (!(forceBook.ContainsKey(forceSide) && forceBook[forceSide].Contains(forceUser)))
                    {
                        forceBook.Add(forceSide, new List<string>());
                        forceBook[forceSide].Add(forceUser);
                    }
                }
                else if (command.Contains(" -> "))
                {
                    string[] line = command.Split(" -> ");
                    string forceUser = line[0];
                    string forceSide = line[1];

                    

                    if (!forceBook[forceSide].Contains(forceUser))
                    {
                        foreach (var item in forceBook)
                        {
                            forceBook[item.Key].Remove(forceUser);
                        }

                        forceBook[forceSide].Add(forceUser);

                        Console.WriteLine($"{forceUser} joins the {forceSide} side!");
                    }
                }

                command = Console.ReadLine();
            }

            var orderForceBook = forceBook.OrderByDescending(x => x.Value.Count).Where(x => x.Value.Count > 0);

            foreach (var item in orderForceBook)//.Where(x => x.Value.Count > 0)
            {
                Console.WriteLine($"Side: {item.Key}, Members: {item.Value.Count()}");

                foreach (var user in item.Value.OrderBy(x => x))
                {
                    Console.WriteLine($"! {user}");
                }
            }
