            string[] names = Console.ReadLine().Split(", ");

            foreach (var name in names)
            {
                bool isValid = false;

                if (name.Length >= 3 && name.Length <= 16)
                {
                    foreach (var chars in name)
                    {
                        if (char.IsLetterOrDigit(chars) || chars == '-' || chars == '_')
                        {
                            isValid = true;
                        }
                        else
                        {
                            isValid = false;
                            break;
                        }
                    }
                }

                if (isValid)
                {
                    Console.WriteLine(name);
                }
            }
